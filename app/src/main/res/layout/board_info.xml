<ScrollView android:layout_width="fill_parent" android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android" >
<LinearLayout style="@style/info_layout">        

    <!-- SN Number -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/sn_number_label" style="@style/info_label" />
        <TextView android:id="@+id/sn_number" style="@style/info_value" />
    </LinearLayout>
	
    <!-- BT/WIFI Flag -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/bt_wifi_flag_label" style="@style/info_label" />
        <TextView android:id="@+id/bt_wifi_flag" style="@style/info_value" />
    </LinearLayout>

    <!-- GSM Flag -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/gsm_flag_label" style="@style/info_label" />
        <TextView android:id="@+id/gsm_flag" style="@style/info_value" />
    </LinearLayout>

    <!-- TD Flag -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/td_flag_label" style="@style/info_label" />
        <TextView android:id="@+id/td_flag" style="@style/info_value" />
    </LinearLayout>
    
    <!-- LTE Flag -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/lte_flag_label" style="@style/info_label" />
        <TextView android:id="@+id/lte_flag" style="@style/info_value" />
    </LinearLayout>

    <!-- Coupling Flag -->
    <LinearLayout style="@style/entry_layout">
        <TextView android:text="@string/coupling_flag_label" style="@style/info_label" />
        <TextView android:id="@+id/coupling_flag" style="@style/info_value" />
    </LinearLayout>


</LinearLayout>
</ScrollView>
